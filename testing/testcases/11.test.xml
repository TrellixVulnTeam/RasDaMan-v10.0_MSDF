<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ProcessCoveragesRequest xmlns='http://www.opengis.net/wcps/1.0'
          schemaLocation='http://schemas.opengis.net/wcps/1.0 wcpsProcessCoverages.xsd' service="WCS" version="1.1.2">
    <query>
        <xmlSyntax>
            <coverageIterator>
                <iteratorVar>s</iteratorVar>
                <coverageName>rgb</coverageName>
            </coverageIterator>
            <encode>
                <format>jpeg</format>
                <cast>
                    <div>
                        <minus>
                            <fieldSelect>
                                <field>
                                    <name>0</name>
                                </field>
                                <coverage>s</coverage>
                            </fieldSelect>
                            <fieldSelect>
                                <field>
                                    <name>1</name>
                                </field>
                                <coverage>s</coverage>
                            </fieldSelect>
                        </minus>
                        <plus>
                            <fieldSelect>
                                <field>
                                    <name>0</name>
                                </field>
                                <coverage>s</coverage>
                            </fieldSelect>
                            <fieldSelect>
                                <field>
                                    <name>1</name>
                                </field>
                                <coverage>s</coverage>
                            </fieldSelect>
                        </plus>
                    </div>
                    <type>char</type>
                </cast>
            </encode>
        </xmlSyntax>
    </query>
</ProcessCoveragesRequest>
