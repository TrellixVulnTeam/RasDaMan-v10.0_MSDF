<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<ns1:ProcessCoveragesRequest 
         xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' 
       xmlns:ns1='http://www.opengis.net/wcps/1.0' 
          xsi:schemaLocation='http://www.opengis.net/wcps/1.0 wcpsProcessCoverage.xsd' service="WCS" version="1.1.2"> 
        <ns1:query>
            <ns1:xmlSyntax>
<ns1:coverageIterator>
        <ns1:iteratorVar>s</ns1:iteratorVar>
        <ns1:coverageName>rgb</ns1:coverageName>
    </ns1:coverageIterator>
    <ns1:encode>
        <ns1:format>jpeg</ns1:format>
        <ns1:div>
            <ns1:minus>
                <ns1:select>
                    <ns1:field>0</ns1:field>
                    <ns1:coverage>s</ns1:coverage>
                </ns1:select>
                <ns1:select>
                    <ns1:field>1</ns1:field>
                    <ns1:coverage>s</ns1:coverage>
                </ns1:select>
            </ns1:minus>
            <ns1:greaterThan>
                <ns1:plus>
                    <ns1:select>
                        <ns1:field>0</ns1:field>
                        <ns1:coverage>s</ns1:coverage>
                    </ns1:select>
                    <ns1:select>
                        <ns1:field>1</ns1:field>
                        <ns1:coverage>s</ns1:coverage>
                    </ns1:select>
                </ns1:plus>
                <ns1:scalar>0.6</ns1:scalar>
            </ns1:greaterThan>
        </ns1:div>
    </ns1:encode>
      </ns1:xmlSyntax>
    </ns1:query>
</ns1:ProcessCoveragesRequest>
