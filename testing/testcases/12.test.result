<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<processCoverage xmlns="http://www.opengis.net/wcps/1.0.0" service="WCPS" version="1.0.0">
<coverageIterator>
        <iteratorVar>s</iteratorVar>
        <coverageName>rgb</coverageName>
    </coverageIterator>
    <encode>
        <format>jpeg</format>
        <div>
            <minus>
                <select>
                    <field>0</field>
                    <coverage>s</coverage>
                </select>
                <select>
                    <field>1</field>
                    <coverage>s</coverage>
                </select>
            </minus>
            <greaterThan>
                <plus>
                    <select>
                        <field>0</field>
                        <coverage>s</coverage>
                    </select>
                    <select>
                        <field>1</field>
                        <coverage>s</coverage>
                    </select>
                </plus>
                <scalar>0.6</scalar>
            </greaterThan>
        </div>
    </encode>
</processCoverage>
