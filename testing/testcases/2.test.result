<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<processCoverage xmlns="http://www.opengis.net/wcps/1.0.0" service="WCPS" version="1.0.0">
<coverageIterator>
        <iteratorVar>c</iteratorVar>
        <coverageName>rgb</coverageName>
    </coverageIterator>
    <encode>
        <format>png</format>
        <cast>
            <type>char</type>
            <trim>
                <trimAxisSpec>
                    <axis>x</axis>
                    <coord>0</coord>
                    <coord>450</coord>
                </trimAxisSpec>
                <trimAxisSpec>
                    <axis>y</axis>
                    <coord>0</coord>
                    <coord>300</coord>
                </trimAxisSpec>
                <abs>
<minus>
                        <select>
                            <field>1</field>
                            <coverage>c</coverage>
                        </select>
                        <select>
                            <field>0</field>
                            <coverage>c</coverage>
                        </select>
                    </minus>
                </abs>
</trim>
        </cast>
    </encode>
</processCoverage>
