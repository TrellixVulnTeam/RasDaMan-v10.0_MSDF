<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<processCoverage xmlns="http://www.opengis.net/wcps/1.0.0" service="WCPS" version="1.0.0">
<coverageIterator>
        <iteratorVar>c</iteratorVar>
        <coverageName>rgb</coverageName>
    </coverageIterator>
    <encode>
        <format>png</format>
        <cast>
            <type>char</type>
            <mult>
                <greaterThan>
                    <minus>
                        <div>
                            <select>
                                <field>0</field>
                                <coverage>c</coverage>
                            </select>
                            <cast>
                                <type>float</type>
                                <plus>
                                    <select>
                                        <field>0</field>
                                        <coverage>c</coverage>
                                    </select>
                                    <select>
                                        <field>1</field>
                                        <coverage>c</coverage>
                                    </select>
                                </plus>
                            </cast>
                        </div>
                        <div>
                            <select>
                                <field>1</field>
                                <coverage>c</coverage>
                            </select>
                            <cast>
                                <type>float</type>
                                <plus>
                                    <select>
                                        <field>0</field>
                                        <coverage>c</coverage>
                                    </select>
                                    <select>
                                        <field>1</field>
                                        <coverage>c</coverage>
                                    </select>
                                </plus>
                            </cast>
                        </div>
                    </minus>
                    <scalar>0.6</scalar>
                </greaterThan>
                <scalar>255</scalar>
           </mult>
        </cast>
    </encode>
</processCoverage>
