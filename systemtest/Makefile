# general system test
.PHONY: check
check:
	for testcase in ${TESTSEQUENCE}; do\
		 RMANBASE=${RMANBASE}\
		 PATH=${bindir}:${PATH}\
		 PROGTEST=$${testcase}/*.sh;\
     $$testcase/*.sh;\
		 if [ $$? -ne 0 ]; \
		 then\
			echo $${PROGTEST} ... FAIL>>log;\
		 else \
		 	echo $${PROGTEST} ... OK>>log;\
		 fi; \
		 echo "	"see detail in $$testcase/log>>log;\
	done;\
	cat log;\
	rm log;

clean:
	for testcase in ${TESTSEQUENCE}; do \
               PATH=${bindir}:${PATH}   rm -rf $${testcase}/log* $${testcase}/failed_cases; \
        done;

