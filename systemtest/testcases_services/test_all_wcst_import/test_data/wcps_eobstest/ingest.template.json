{
  "config": {
    "service_url": "PETASCOPE_URL",
    "tmp_directory": "/tmp/",
    "crs_resolver": "SECORE_URL/",
    "default_crs": "SECORE_URL/crs/EPSG/0/4326",
    "mock": false,
    "automated": true,
    "track_files": false,
    "subset_correction": true
  },
  "input": {
    "coverage_id": "test_eobstest",
    "paths": [
      "eobs.nc"
    ]
  },
  "recipe": {
    "name": "general_coverage",
    "options": {
      "coverage": {
        "crs": "EPSG/0/4326@OGC/0/AnsiDate?axis-label=\"t\"",
        "metadata": {
          "type": "xml",
          "global": {
            "Title": "'This is a test file'",
            "Project": "'This is another test file'",
            "Creator": "'This is a test creator file'"
          }
        },
        "slicer": {
          "type": "netcdf",
          "bands": [
            {
              "name": "tg",
              "variable": "tg",
              "description": "Count of the number of observations from the MERIS sensor contributing to this bin cell",
              "nilReason": "The nil value represents an error in the sensor.",
              "identifier": "tg"
            }
          ],
          "axes": {
            "t": {
	      "min": "datetime('19500101:0000', 'YYYYMMDD:HHmm')",
              "max": "datetime('19500106:0000', 'YYYYMMDD:HHmm')",
              "resolution": 1,
              "gridOrder": 0,
              "type": "ansidate",
              "irregular": false
            },	    
            "Lat": {
	      "min": "25.0",
              "max": "75.5",
              "gridOrder": 1,
              "resolution": "-0.5"
            },
            "Long": {              
              "min": "-40.5",
              "max": "75.5",
              "gridOrder": 2,
              "resolution": "0.5"
            }
          }
        }
      },
      "tiling": "ALIGNED [0:0, 0:1023, 0:1023]"
    }
  }
}
