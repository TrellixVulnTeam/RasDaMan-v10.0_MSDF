<?xml version="1.0" encoding="UTF-8"?>
<wcs:GetCoverage
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:wcs="http://www.opengis.net/wcs/2.0"
    xmlns:gml="http://www.opengis.net/gml/3.2"
    xmlns:scal="http://www.opengis.net/wcs/scaling/1.0"
    xmlns:int="http://www.opengis.net/wcs/interpolation/1.0"
    xsi:schemaLocation="http://schemas.opengis.net/wcs/2.0 ../wcsAll.xsd"
    service="WCS" version="2.0.1">
  <wcs:CoverageId>test_mr</wcs:CoverageId>
  <wcs:Extension>
    <scal:Scaling>
      <scal:ScaleByFactor>
        <scal:scaleFactor>2</scal:scaleFactor>
      </scal:ScaleByFactor>
    </scal:Scaling>
    <int:Interpolation>
      <int:globalInterpolation>http://www.opengis.net/def/interpolation/OGC/0/nearest-neighbor</int:globalInterpolation>
    </int:Interpolation>
  </wcs:Extension>
</wcs:GetCoverage>
