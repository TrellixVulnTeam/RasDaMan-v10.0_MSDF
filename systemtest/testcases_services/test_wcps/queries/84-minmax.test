for c in (mean_summer_airtemp)
  return 
        (condense min
        over     $py1 y (300:301)
        using    c[Long:"http://localhost:8080/def/crs/OGC/0/Index2D/"(100), Lat:"http://localhost:8080/def/crs/OGC/0/Index2D/"($py1)])
        +
        (condense max
        over     $py2 y (300:301)
        using    c[Long:"http://localhost:8080/def/crs/OGC/0/Index2D/"(100), Lat:"http://localhost:8080/def/crs/OGC/0/Index2D/"($py2)])
