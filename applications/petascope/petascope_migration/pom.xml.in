<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <groupId>org.rasdaman</groupId>
    <artifactId>petascope-migration</artifactId>
    <packaging>jar</packaging>
    <name>petascope-migration</name>
    <description>Petascope migration application</description>
    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.version>1.7</java.version>
        <!-- to disable error when generating javadoc for java 1.8 -->
        <additionalparam>-Xdoclint:none</additionalparam>
        <!-- The main class to start for Spring Boot-->
        <start-class>org.rasdaman.ApplicationMigration</start-class>
    </properties>
    <parent>
        <groupId>org.rasdaman</groupId>
        <artifactId>petascope-parent</artifactId>
        <version>@PROJECT_VERSION@</version>
    </parent>
    <dependencies>
    </dependencies>
    <build>
        <finalName>rasdaman-geo-migration</finalName>
        <!-- The built package should be in build directory, not in source directory -->
        <directory>@PETASCOPE_TARGET_DIR@/petascope_migration</directory>
    </build>
</project>
